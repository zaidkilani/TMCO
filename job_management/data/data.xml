<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data >
        <record id="seq_siv" model="ir.sequence">
            <field name="name">siv seq</field>
            <field name="code">siv.seq</field>
            <field name="prefix">SIV/%(y)s/%(month)s/%(day)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False" />
        </record>
        
         <record id="seq_job" model="ir.sequence">
            <field name="name">job seq</field>
            <field name="code">job.seq</field>
            <field name="prefix">JOB/%(y)s/%(month)s/%(day)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False" />
        </record>
    </data>

</odoo>